{
  "redirects": [
    { 
      "source": "/page3",
      "destination": "/page2", 
      "permanent": true
    },
    {
      "source":"/(.*)",
      "statusCode": 404,
      "destination": "/custom404Page/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "DENY"
        },
        {
          "key" : "X-XSS-Protection",
          "value" : "1; mode=block"
        },
        {
          "key" : "Referrer-Policy",
          "value" : "strict-origin"
        },
        {
          "key" : "Permissions-Policy",
          "value" : "geolocation=(self), microphone=()"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": true
}
